        <Label x:Name="message" HorizontalContentAlignment="Center" HorizontalAlignment="Center" Content="Invalid Command" Foreground="Black" FontWeight="Bold" FontFamily="Tahoma" FontSize="20" Margin="31,63,28,112" RenderTransformOrigin="0.5,0.5" Width="233"/>
